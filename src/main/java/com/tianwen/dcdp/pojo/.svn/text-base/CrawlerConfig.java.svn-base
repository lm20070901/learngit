package com.tianwen.dcdp.pojo;

public class CrawlerConfig {
    private Long id;

    private String websiteName;

    private String websiteAdress;

    private String isPaging;

    private String pagingRules;

    private Integer threadNumber;

    private Integer collectInterval;

    private String filterRegular;

    private String isUseAgent;

    private String headOptions;

    private Integer depth;

    private Long wId;

    private String fieldConfig;
    
    //模版名称
  	private String modelName;
  	//类路径
  	private String modelClassPath;
  	//模版备注
  	private String bz;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

	public String getWebsiteName() {
		return websiteName;
	}

	public void setWebsiteName(String websiteName) {
		this.websiteName = websiteName;
	}

	public String getWebsiteAdress() {
		return websiteAdress;
	}

	public void setWebsiteAdress(String websiteAdress) {
		this.websiteAdress = websiteAdress;
	}

	public String getIsPaging() {
		return isPaging;
	}

	public void setIsPaging(String isPaging) {
		this.isPaging = isPaging;
	}

	public String getPagingRules() {
		return pagingRules;
	}

	public void setPagingRules(String pagingRules) {
		this.pagingRules = pagingRules;
	}

	public Integer getThreadNumber() {
		return threadNumber;
	}

	public void setThreadNumber(Integer threadNumber) {
		this.threadNumber = threadNumber;
	}

	public Integer getCollectInterval() {
		return collectInterval;
	}

	public void setCollectInterval(Integer collectInterval) {
		this.collectInterval = collectInterval;
	}

	public String getFilterRegular() {
		return filterRegular;
	}

	public void setFilterRegular(String filterRegular) {
		this.filterRegular = filterRegular;
	}

	public String getIsUseAgent() {
		return isUseAgent;
	}

	public void setIsUseAgent(String isUseAgent) {
		this.isUseAgent = isUseAgent;
	}

	public String getHeadOptions() {
		return headOptions;
	}

	public void setHeadOptions(String headOptions) {
		this.headOptions = headOptions;
	}

	public Integer getDepth() {
		return depth;
	}

	public void setDepth(Integer depth) {
		this.depth = depth;
	}

	public Long getwId() {
		return wId;
	}

	public void setwId(Long wId) {
		this.wId = wId;
	}

	public String getFieldConfig() {
		return fieldConfig;
	}

	public void setFieldConfig(String fieldConfig) {
		this.fieldConfig = fieldConfig;
	}

	public String getModelName() {
		return modelName;
	}

	public void setModelName(String modelName) {
		this.modelName = modelName;
	}

	public String getModelClassPath() {
		return modelClassPath;
	}

	public void setModelClassPath(String modelClassPath) {
		this.modelClassPath = modelClassPath;
	}

	public String getBz() {
		return bz;
	}

	public void setBz(String bz) {
		this.bz = bz;
	}

    
